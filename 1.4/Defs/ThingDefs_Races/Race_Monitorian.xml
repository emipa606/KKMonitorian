  <!-- Monitorians-->
  <AlienRace.ThingDef_AlienRace ParentName="BasePawn">
    <defName>Alien_Monitorian</defName>
    <label>Monitorian</label>
    <description>A strong and nimble People. The males appear mostly as strong warriors but because of their agressiveness they are often seen fighting for dominance, while the weaker but more numerous females use their elegance and charm in Diplomacy. While they are venomous, they prefer to use their whip like tails or three fingerd claws in hand to hand combat</description>
    <alienRace>
      <generalSettings>
        <humanRecipeImport>true</humanRecipeImport>
        <maleGenderProbability>0.25</maleGenderProbability>
        <immuneToAge>false</immuneToAge>
        <canLayDown>true</canLayDown>
        <forcedRaceTraitEntries>
          <li>
            <defName>Bloodlust</defName>
            <chance>50</chance>
            <commonalityMale>50</commonalityMale>
            <commonalityFemale>10</commonalityFemale>
          </li>
          <li>
            <defName>Beauty</defName>
            <degree>2</degree>
            <chance>50</chance>
            <commonalityFemale>20</commonalityFemale>
          </li>
        </forcedRaceTraitEntries>
        <factionRelations>
          <li>
            <factions>
              <li>MonitorianFaction</li>
              <li>MonitorianPlayerTribe</li>
            </factions>
            <goodwill>
              <min>99</min>
              <max>99</max>
            </goodwill>
          </li>
        </factionRelations>
        <alienPartGenerator>
          <headTypes Inherit="False">
            <li>Monitorian</li>
          </headTypes>
          <bodyTypes Inherit="False">
            <li>Male</li>
            <li>Female</li>
          </bodyTypes>
          <colorChannels>
            <li>
              <name>skin</name>
              <first Class="ColorGenerator_Options">
                <options>
                  <!--  White -->
                  <li>
                    <weight>1</weight>
                    <min>RGBA(0.9,0.9,0.90,1)</min>
                    <max>RGBA(1.0,1.0,1.0,1)</max>
                  </li>
                  <!--  Darkish -->
                  <li>
                    <weight>10</weight>
                    <min>RGBA(0.4,0.2,0.2,1)</min>
                    <max>RGBA(0.8,0.5,0.4,1)</max>
                  </li>
                  <!--  Reddish -->
                  <li>
                    <weight>5</weight>
                    <min>RGBA(0,8,0.1,0.15,1)</min>
                    <max>RGBA(0.9,0.5,0.28,1)</max>
                  </li>
                  <!--  Greenish -->
                  <li>
                    <weight>5</weight>
                    <min>RGBA(0.20,0.60,0.20,1)</min>
                    <max>RGBA(0.50,0.6,0.3,1)</max>
                  </li>
                  <!--  Blueish -->
                  <li>
                    <weight>5</weight>
                    <min>RGBA(0.20,0.50,0.70,1)</min>
                    <max>RGBA(0.50,0.6,0.8,1)</max>
                  </li>
                  <!--  Pinkish -->
                  <li>
                    <weight>5</weight>
                    <min>RGBA(0.60,0.40,0.70,1)</min>
                    <max>RGBA(0.80,0.5,0.8,1)</max>
                  </li>
                </options>
              </first>
            </li>
            <li>
              <name>hair</name>
              <first Class="ColorGenerator_Options">
                <options>
                  <!-- Pale -->
                  <li>
                    <weight>5</weight>
                    <min>RGBA(0.9,0.9,0.90,1)</min>
                    <max>RGBA(1.0,1.0,1.0,1)</max>
                  </li>
                </options>
              </first>
            </li>
          </colorChannels>
          <customDrawSize>(1.5, 1.5)</customDrawSize>
          <customHeadDrawSize>(1.0, 1.0)</customHeadDrawSize>
          <customPortraitDrawSize>(1, 1)</customPortraitDrawSize>
          <customPortraitHeadDrawSize>(0.66, 0.66)</customPortraitHeadDrawSize>
        </alienPartGenerator>
      </generalSettings>
      <graphicPaths>
        <body>Things/Pawn/Humanlike/Monitorian/Body/</body>
        <head>Things/Pawn/Humanlike/Monitorian/Head/</head>
      </graphicPaths>
      <styleSettings>
        <li>
          <key>HairDef</key>
          <value>
            <styleTags>
              <li>MonitorianHair</li>
            </styleTags>
          </value>
        </li>
        <li>
          <key>BeardDef</key>
          <value>
            <hasStyle>false</hasStyle>
          </value>
        </li>
      </styleSettings>
      <thoughtSettings>
        <cannotReceiveThoughts>
          <li>SleptOnGround</li>
        </cannotReceiveThoughts>
      </thoughtSettings>
      <relationSettings>
      </relationSettings>
      <raceRestriction>
        <weaponList>
          <li>MeleeWeapon_Hammer</li>
          <li>Rock</li>
          <li>Shell</li>
        </weaponList>
        <whiteWeaponList>
          <li>MeleeWeapon_Hammer</li>
          <li>Rock</li>
          <li>Shell</li>
          <li>Pila</li>
          <li>MeleeWeapon_Ikwa</li>
          <li>MeleeWeapon_Spear</li>
          <li>MeleeWeapon_Club</li>
          <li>Weapon_GrenadeFrag</li>
          <li>Weapon_GrenadeMolotov</li>
          <li>Weapon_GrenadeEMP</li>
        </whiteWeaponList>
        <blackGeneList>
          <li MayRequire="Ludeon.RimWorld.Biotech">Brow_Heavy</li>
          <li MayRequire="Ludeon.RimWorld.Biotech">FacialRidges</li>
        </blackGeneList>
        <blackGeneTags>
          <li>SkinColor</li>
          <li>HairColor</li>
          <li>EyeColor</li>
          <li>BodyType</li>
          <li>Ears</li>
          <li>Nose</li>
          <li>Jaw</li>
          <li>Head</li>
          <li>Headbone</li>
          <li>HairStyle</li>
          <li>BeardStyle</li>
          <li>SkinColorOverride</li>
          <li>Fur</li>
          <li>Tail</li>
        </blackGeneTags>
        <blackEndoCategories>
          <li>Melanin</li>
          <li>HairColor</li>
          <li>Ears</li>
          <li>Nose</li>
          <li>Jaw</li>
          <li>Head</li>
          <li>BodyType</li>
        </blackEndoCategories>
      </raceRestriction>
    </alienRace>
    <statBases>
      <MarketValue>1500</MarketValue>
      <MoveSpeed>4.5</MoveSpeed>
      <Flammability>1.0</Flammability>
      <ArmorRating_Blunt>0.1</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.3</ArmorRating_Sharp>
      <ComfyTemperatureMin>20</ComfyTemperatureMin>
      <ComfyTemperatureMax>40</ComfyTemperatureMax>
      <LeatherAmount>40</LeatherAmount>
      <ToxicResistance>-0.8</ToxicResistance>
      <CarryingCapacity>150</CarryingCapacity>
      <MeleeDodgeChance>1.25</MeleeDodgeChance>
      <ShootingAccuracyPawn>0.9</ShootingAccuracyPawn>
      <AimingDelayFactor>1</AimingDelayFactor>
      <WorkSpeedGlobal>0.8</WorkSpeedGlobal>
      <ResearchSpeed>0.5</ResearchSpeed>
    </statBases>
    <tools>
      <li>
        <label>left claws</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.60</cooldownTime>
        <linkedBodyPartsGroup>LeftHandClawsGroup</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>12</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right claws</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.60</cooldownTime>
        <linkedBodyPartsGroup>RightHandClawsGroup</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>12</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>teeth</label>
        <capacities>
          <li>ToxicBite</li>
        </capacities>
        <power>13</power>
        <cooldownTime>1.80</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.5</chanceFactor>
        <soundMeleeHit>Pawn_Melee_HumanBite_Hit</soundMeleeHit>
        <soundMeleeMiss>Pawn_Melee_HumanBite_Miss</soundMeleeMiss>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
      <li>
        <label>tail</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>15</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Tail</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.5</chanceFactor>
      </li>
    </tools>
    <race>
      <thinkTreeMain>Humanlike</thinkTreeMain>
      <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
      <intelligence>Humanlike</intelligence>
      <makesFootprints>true</makesFootprints>
      <leatherDef>Leather_Lizard</leatherDef>
      <lifeExpectancy>120</lifeExpectancy>
      <nameCategory>HumanStandard</nameCategory>
      <body>MonitorianBody</body>
      <baseBodySize>1.5</baseBodySize>
      <baseHealthScale>1.5</baseHealthScale>
      <foodType>OmnivoreHuman</foodType>
      <gestationPeriodDays>45</gestationPeriodDays>
      <litterSizeCurve>
        <points>
          <li>(0.5, 0)</li>
          <li>(1, 1)</li>
          <li>(1.01, 0.02)</li>
          <li>(3.5, 0)</li>
        </points>
      </litterSizeCurve>
      <lifeStageAges>
        <li>
          <def>MonitorianSeedlingStage</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>MonitorianYoungStage</def>
          <minAge>2</minAge>
        </li>
        <li>
          <def>MonitorianAdolescentStage</def>
          <minAge>15</minAge>
        </li>
        <li>
          <def>MonitorianAdultStage</def>
          <minAge>30</minAge>
        </li>
        <li>
          <def>MonitorianElderStage</def>
          <minAge>120</minAge>
        </li>
        <li>
          <def>MonitorianAncientStage</def>
          <minAge>140</minAge>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_Punch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_Punch_Miss</soundMeleeMiss>
      <specialShadowData>
        <volume>(0.3, 0.8, 0.4)</volume>
        <offset>(0,0,-0.3)</offset>
      </specialShadowData>
      <ageGenerationCurve>
        <points>
          <li>(16,0)</li>
          <li>(20,50)</li>
          <li>(50,100)</li>
          <li>(100,70)</li>
          <li>(110,18)</li>
          <li>(120,10)</li>
          <li>(130,3)</li>
